<view class="month_background">
  <view class="month_title">{{month_name}}</view>
  <view class="month_cell_container">
    <view class="day_name_container">
      <view class="day_name" wx:for="{{['日','一','二','三','四','五','六']}}" wx:key="name">{{item}}</view>
    </view>
    <view class="days_container">
      <view class="days" wx:for="{{month_days}}" wx:key="days">
        {{item}}</view>
    </view>
  </view>
</view>